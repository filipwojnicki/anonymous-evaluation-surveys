{
  "name": "@apps/source",
  "version": "0.0.0",
  "private": true,
  "license": "MIT",
  "workspaces": [
    "packages/*",
    "frontend",
    "backend-e2e",
    "backend"
  ],
  "scripts": {
    "codegen": "graphql-codegen --config codegen.ts",
    "precommit": "nx affected -t lint format test --parallel=5",
    "commit": "git add --all && cz",
    "lint": "nx run-many --target=lint --all",
    "prepare": "cd .. && husky install apps/.husky",
    "sort-package-json": "npx sort-package-json",
    "start:backend:dev": "nx serve backend",
    "start:frontend:dev": "nx serve frontend",
    "build:backend": "nx reset && nx build backend",
    "build:frontend": "nx reset && nx build frontend",
    "test": "nx run-many --target=test --all"
  },
  "config": {
    "commitizen": {
      "path": "./apps/node_modules/cz-git"
    }
  },
